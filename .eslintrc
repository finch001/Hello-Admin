{
  "extends": ["airbnb","prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "generator-star-spacing": "off",
    "linebreak-style": [
        "error",
        "windows"
    ]
  },
  "env":{
    "browser": true,
    "es6": true
  }
}
